import java.util.*;
class Main {
  public static void main(String[] args) {
    Scanner sc=new Scanner(System.in);
    double n=sc.nextInt();
    double s=Math.sqrt(n);
    if(s*s==n){
        System.out.println("Perfect Square");
    }
    else{
      System.out.println("Not a Perfect Square");
    }
  }
}  
/*  4 
    Perfect Square 
    8 
    Not a Perfect Square  */
